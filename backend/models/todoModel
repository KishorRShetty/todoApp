import { Schema, model } from "mongoose";

const todoSchema = new Schema({
  name: {
    type: String,
    required: [true, "Please enter the title"],
  },
  description: {
    type: String,
  },
  createdAt: {
    type: Date,
    default: Date.now(),
  },
  updatedAt: {
    type: Date,
    default: Date.now(),
  },
});

export default model("lists", todoSchema);
